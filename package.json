{
  "name": "git-oneflow",
  "version": "1.1.0",
  "description": "CLI tooling implementing GIT OneFlow branching model",
  "bin": {
    "gof": "bin/cli",
    "git-oneflow": "bin/cli"
  },
  "files": [
    "bin/cli"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "tsc && jest",
    "posttest": "npm run format",
    "clean": "rimraf bin",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "typecheck": "tsc",
    "prerelease": "GOF_DRY_RUN=true npm test",
    "release": "standard-version",
    "prepublishOnly": "npm test && npm run build",
    "lint": "eslint . --cache --fix --ext .ts",
    "format": "prettier --loglevel warn --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msanguineti/git-oneflow.git"
  },
  "keywords": [
    "git",
    "oneflow",
    "branching",
    "model",
    "Adam",
    "Ruka"
  ],
  "author": "Mirco Sanguineti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msanguineti/git-oneflow/issues"
  },
  "homepage": "https://github.com/msanguineti/git-oneflow#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "rollup": "^2.33.2",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "cosmiconfig": "^7.0.0",
    "inquirer": "^7.3.3",
    "shelljs": "^0.8.4"
  }
}
